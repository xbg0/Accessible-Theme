    "semanticHighlighting": true,
    "tokenColors": [
        {
            "name": "None Style",
            "scope": [
                "text.html.markdown constant",
            ],
            "settings": {
                "foreground": "#8895A0"
            }
        },
        {
            "name": "FFFFFF8F",
            "scope": [
                "string.other.link.title.markdown",
                "string.other.link.description.markdown",
                "string.other.link.description.title.markdown",
                "constant.other.reference.link.markdown",
                "meta.embedded.line.ruby",
                "variable.parameter.function.ruby",
                "keyword.operator.pipe.shell"
            ],
            "settings": {
                "foreground": "#FFFFFF8F"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.markdown",
                "punctuation.definition.quote.begin.markdown",
                "fenced_code.block.language.markdown",
                "meta.separator.markdown"
            ],
            "settings": {
                "foreground": "#8895A04D"
            }
        },
        {
            "name": "Purple:Comment",
            "scope": [
                "comment storage"
            ],
            "settings": {
                "foreground": "#ba8adeb0"
            }
        },
        {
            "name": "Yellow:Comment",
            "scope": [
                "comment entity.name.type",
            ],
            "settings": {
                "foreground": "#D1BC71b0"
            }
        },
        {
            "name": "Red:Comment",
            "scope": [
                "comment variable",
            ],
            "settings": {
                "foreground": "#DB676Bb0"
            }
        },
        {
            "name": "Blue:Comment",
            "scope": [
                "comment punctuation.definition.block",
            ],
            "settings": {
                "foreground": "#5EA8DCB0"
            }
        },
        {
            "name": "Purple:First",
            "scope": [
                "keyword",
                "storage",
                "storage.type.class.js",
                "string.regexp",
                "meta.var.expr.js storage.type.js",
                "entity.other.attribute-name.pseudo-element.css",
                "punctuation.definition.link.title.begin.markdown",
                "punctuation.definition.link.title.end.markdown",
                "punctuation.definition.link.description.begin.markdown",
                "punctuation.definition.link.description.end.markdown",
