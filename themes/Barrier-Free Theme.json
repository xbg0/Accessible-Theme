            "settings": {
                "foreground": "#8DB5CA"
            }
        },
        {
            "name": "Red:First",
            "scope": [
                "variable.language.this",
                "keyword.operator.new",
                "keyword.other.new"
            ],
            "settings": {
                "foreground": "#E04A63"
            }
        },
        {
            "name": "Red:Second",
            "scope": [
                "variable",
                "entity.name.tag",
                "constant.other.character-class.regexp",
                "meta.function.c",
                "entity.name.variable.field.cs",
                "entity.name.variable.local.cs",
